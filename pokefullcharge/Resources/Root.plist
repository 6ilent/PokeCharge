<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
		</dict>

		<dict>
				<key>cell</key><string>PSStaticTextCell</string>
				<key>height</key><string>135</string>
				<key>label</key><string></string>
				<key>icon</key><string>banner.png</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Settings should apply without the need of a respring! :D</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Health Up Settings</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><false/>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>label</key><string>Enable Me!</string>
			<key>key</key><string>enabled</string>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key><string>PSEditTextCell</string>
			<key>default</key><integer>100</integer>
			<key>isNumeric</key><true/>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>detail</key><string>PSDetailController</string>
			<key>key</key><string>batPercent</string>
			<key>label</key><string>Battery Percent:</string>
			<key>placeholder</key><string>0 - 100</string>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key><string>PSLinkListCell</string>
			<key>key</key><string>sound</string>
			<key>default</key><integer>0</integer>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
			<key>label</key><string>Health Up Sound:</string>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>detail</key><string>PSListItemsController</string>
			<key>validTitles</key>
			<array>
				<string>Fully Healed</string>
				<string>Critical Health</string>
				<string>Metal Gear Solid: Alert!</string>
				<string>Switch: Bing</string>
				<string>Switch: Enter</string>
				<string>Switch: User</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
				<integer>3</integer>
				<integer>4</integer>
				<integer>5</integer>
			</array>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Low Health Settings</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><false/>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>label</key><string>Enable Me!</string>
			<key>key</key><string>lowenabled</string>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key><string>PSEditTextCell</string>
			<key>default</key><integer>20</integer>
			<key>isNumeric</key><true/>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>detail</key><string>PSDetailController</string>
			<key>key</key><string>lowbatPercent</string>
			<key>label</key><string>Battery Percent:</string>
			<key>placeholder</key><string>0 - 100</string>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key><string>PSLinkListCell</string>
			<key>key</key><string>lowsound</string>
			<key>default</key><integer>1</integer>
			<key>PostNotification</key><string>com.6ilent.pokefullcharge/ReloadPrefs</string>
			<key>label</key><string>Low Health Sound:</string>
			<key>defaults</key><string>com.6ilent.pokefullcharge</string>
			<key>detail</key><string>PSListItemsController</string>
			<key>validTitles</key>
			<array>
				<string>Fully Healed</string>
				<string>Critical Health</string>
				<string>Metal Gear Solid: Alert!</string>
				<string>Switch: Bing</string>
				<string>Switch: Enter</string>
				<string>Switch: User</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
				<integer>3</integer>
				<integer>4</integer>
				<integer>5</integer>
			</array>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Contact &amp; Support</string>
		</dict>

		<dict>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Follow me on Twitter!</string>
			<key>icon</key><string>twitter.png</string>
			<key>action</key><string>openTwitter</string>
		</dict>

		<dict>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Add me on Reddit!</string>
			<key>icon</key><string>reddit.png</string>
			<key>action</key><string>openReddit</string>
		</dict>

		<dict>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Buy Me a Drink! (Paypal)</string>
			<key>icon</key><string>paypal.png</string>
			<key>action</key><string>openPaypal</string>
		</dict>

		<dict>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Source Code!</string>
			<key>icon</key><string>github.png</string>
			<key>action</key><string>openGithub</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>footerAlignment</key><integer>0</integer>
			<key>label</key><string>Made by 6ilent Â© 2018</string>
		</dict>

	</array>
	<key>title</key>
	<string>PokeFullCharge Preferences</string>
</dict>
</plist>
